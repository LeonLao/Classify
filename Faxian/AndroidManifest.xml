<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.faxian"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />
    <!--  允许程序访问有关GSM网络信息 (全球移动通信)-->
	<uses-permission android:name="android.permission.ACCESS_ENTWORK_STATE"/>


<!--  允许程序打开网络套接字 -->
	<uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/mume_left"
        android:label=""
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
           
             >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name=".WebPage" android:theme="@android:style/Theme.NoTitleBar"></activity>
    </application>

</manifest>
